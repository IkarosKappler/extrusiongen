  <script>
    function changeColor( c ) {
          document.forms["color_form"].elements["color"].value = c;
    
          // Convert color string to R-G-B values  
          previewCanvasHandler.setMaterialColorRGB( c,
                                                    true // redraw
                                                    );
    }
  </script>
